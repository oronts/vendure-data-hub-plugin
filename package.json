{
  "name": "@oronts/vendure-data-hub-plugin",
  "version": "0.1.0",
  "description": "A comprehensive ETL (Extract, Transform, Load) data integration plugin for Vendure e-commerce",
  "author": {
    "name": "Oronts Website",
    "email": "office@oronts.com",
    "url": "https://oronts.com"
  },
  "contributors": [
    {
      "name": "Refaat Al Ktifan",
      "email": "Refaat@alktifan.com"
    }
  ],
  "license": "PolyForm-Noncommercial-1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dashboard",
    "docs",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "CONTRIBUTING.md"
  ],
  "keywords": [
    "vendure",
    "vendure-plugin",
    "etl",
    "data-integration",
    "pipeline",
    "data-hub",
    "product-feed",
    "google-shopping",
    "facebook-catalog",
    "amazon-feed",
    "import",
    "export",
    "transform"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oronts/data-hub-plugin.git"
  },
  "homepage": "https://oronts.com",
  "bugs": {
    "url": "https://github.com/oronts/data-hub-plugin/issues"
  },
  "scripts": {
    "dev": "npm run build:dev && npm run start",
    "start": "ts-node dev-server/index.ts",
    "populate": "ts-node dev-server/populate.ts",
    "build": "rimraf dist && npm run compile && npm run build:dashboard",
    "build:dev": "vite build --config vite.dev.config.mts",
    "build:dashboard": "vite build",
    "compile": "tsc -p tsconfig.build.json",
    "compile:watch": "tsc -p tsconfig.build.json --watch",
    "clean": "rimraf dist .dashboard-build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@vendure/core": "^3.0.0",
    "@vendure/dashboard": "^3.0.0",
    "typeorm": "^0.3.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@xyflow/react": "^12.10.0",
    "basic-ftp": "^5.0.0",
    "fast-xml-parser": "^5.0.0",
    "js-yaml": "^4.1.0",
    "multer": "^2.0.0",
    "mysql2": "^3.11.0",
    "nodemailer": "^6.9.0",
    "papaparse": "^5.4.0",
    "pg": "^8.13.0",
    "ssh2-sftp-client": "^11.0.0",
    "xlsx": "^0.18.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/js-yaml": "^4.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.6",
    "@types/nodemailer": "^6.4.0",
    "@types/papaparse": "^5.3.0",
    "@types/pg": "^8.11.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/ssh2-sftp-client": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vendure/asset-server-plugin": "^3.5.2",
    "@vendure/core": "^3.5.2",
    "@vendure/dashboard": "^3.5.2",
    "@vendure/testing": "^3.5.2",
    "@vitejs/plugin-react": "^5.1.2",
    "better-sqlite3": "^12.6.0",
    "eslint": "^9.39.2",
    "graphql-tag": "^2.12.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
